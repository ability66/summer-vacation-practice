<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<!--
    namespace:名称空间
-->
<mapper namespace="com.system.summervacationpractice.mapper.SubjectMapper">
    <!--设置课程信息-->
    <insert id="insertSubject">
        insert into subject values (#{sub_no},#{tea_no},#{sub_name})
    </insert>
    <!--根据工号查课程号-->
    <select id="selectSub_noByTea_no" resultType="java.lang.Integer">
        select sub_no from subject where tea_no=#{tea_no}
    </select>
</mapper>